{
  "name": "subnet-validator-backend",
  "version": "1.0.0",
  "description": "Express backend for tracking Bittensor subnet validation processes",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "watch": "tsc --watch",
    "dev:watch": "concurrently \"npm run watch\" \"nodemon dist/server.js\"",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "bittensor",
    "subnet",
    "validator",
    "tracking",
    "express",
    "typescript",
    "mongodb"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.0.0",
    "cors": "^2.8.5",
    "uuid": "^9.0.0",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "ts-node": "^10.9.1",
    "@types/express": "^4.17.17",
    "@types/node": "^20.0.0",
    "@types/cors": "^2.8.13",
    "@types/uuid": "^9.0.2",
    "nodemon": "^2.0.22",
    "concurrently": "^8.0.0",
    "eslint": "^8.40.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "prettier": "^2.8.8"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}